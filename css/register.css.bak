@font-face{
	font-family: 'Pretendard';
	src:url('../font/PretendardVariable.woff2') format('woff2'),
	    url('../font/PretendardVariable.woff2') format('truetype');
}
body{ 
	font-family:'pretendard';
	color:#0033ff;
	background-image: url("../image/top_bg.png");
	background-repeat: no-repeat;
	background-size:100vw 100vh;
}
header{
	height: 100px;
	margin: 8px 16px 0px;
	line-height: 40px;
}
header #logo{
	float: left;
}
header #logo h1{
	font-size: 0px;
	background-image: url("../image/logo.png");
	background-size: 146px 68px;
	background-repeat: no-repeat;
	background-position: center;
	display: inline-block;
	width: 180px;
	height: 80px;
}

.contain{ 
	margin:0px auto;
	border:1px solid black;
	width:80vw;
	height:70vh;
	/*  column-count:2;
	column-gap:32px;
	column-width:250px;*/
	
}
form{ 
	
	height:100%; 
}
table{ 
	text-align:center;
	float:left; 
	height:100%;
	width:50%;
	margin:0px auto;
}

iframe{ 
width:100%;
height:80%;
}
.terms_box{ 
text-align:center;
width:49%;
height:100%;
float:left;
}

form input{ 
	border-radius:5px; 
	text-align:center;
	border:2px solid #cccccc;
}
form input:focus{ 
	outline:none;
	border:1px solid red;
	box-shadow:0 0 10px red;
	background-color:#dadada; 
	border-radius:5px; 
}
form input[type=text]{ 
}
form input[type=submit]{ 
	position:relative;
	border:1px solid darkred;
	background:gray;
	color:darkred;
}
form input[type=submit]:disabled{ 
	background:gray;
	opacity:0.5;
}
form input[type=submit]:enabled:hover{ 
	cursor:pointer;
	background:black;
}
.terms_checkbox{ 
	margin:10%; 
}
.check_phone{ 
	 margin:0px;
	 padding:0px;
}